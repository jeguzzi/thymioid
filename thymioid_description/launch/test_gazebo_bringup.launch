<?xml version="1.0"?>
<launch>

  <env name="GAZEBO_MODEL_PATH" value="$(find thymio_description)/..:$(find thymioid_description)/..:$(optenv GAZEBO_MODEL_PATH)" />
  <env name="SVGA_VGPU10" value="0"/>
  <arg name="gui" default="true"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="gui" value="$(arg gui)"/>
    <arg name="headless" value="$(eval not arg('gui'))"/>
    <arg name="paused" value="true"/>
    <!-- <arg name="world_name" value="worlds/empty_world"/> -->
  </include>

  <include file="$(find thymioid_description)/launch/gazebo_bringup.launch">
    <arg name="name" value="thymioX"/>
    <arg name="pose" value="-x 0 -y 0 -z 0"/>
    <arg name="camera_is_fixed" value="true"/>
    <arg name="camera_is_on" value="false"/>
    <arg name="publish_ground_truth" value="true"/>
    <arg name="left_wheel_friction" default="1"/>
    <arg name="right_wheel_friction" default="1"/>
    <!-- <arg name="wheel_kp" default="100000"/>
    <arg name="body_kp" default="175000"/> -->
  </include>

  <!-- <include file="$(find thymioid_description)/launch/gazebo_bringup.launch">
    <arg name="name" value="thymio11"/>
    <arg name="pose" value="-x 2 -y 2 -z 0 -yaw 1.3"/>
  </include>

  <include file="$(find thymio_description)/launch/joy_teleop.launch">
    <arg name="namespace" value="thymio10"/>
  </include> -->

  <!-- <include file="$(find thymioid_description)/launch/gazebo_bringup.launch">
    <arg name="name" value="thymio20"/>
    <arg name="pose" value="-x 1 -y 2 -z 0"/>
  </include> -->

</launch>
